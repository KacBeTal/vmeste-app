{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "ALLOW-FROM https://ok.ru"
        },
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors https://ok.ru https://*.ok.ru;"
        }
      ]
    }
  ]
}
